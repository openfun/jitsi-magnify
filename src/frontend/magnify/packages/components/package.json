{
  "name": "@openfun/magnify-components",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006 -s public",
    "react-router-dom": "6.4.1",
    "test": "vitest run",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "compile-translations": "./i18n/compile-translations.cjs",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src"
  },
  "dependencies": {
    "@magnify/typescript-configs": "*",
    "eslint-config-custom": "*",
    "keycloak-js": "20.0.1"
  },
  "peerDependencies": {
    "@jitsi/react-sdk": "1.1.0",
    "@tanstack/react-query": "4.7.2",
    "@types/styled-components": "5.1.25",
    "axios": "0.27.2",
    "formik": "2.2.9",
    "grommet": "2.27.0",
    "grommet-icons": "4.8.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.4",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5",
    "validator": "13.7.0",
    "yup": "0.32.11",
    "@fontsource/roboto": "4.5.8"
  },
  "devDependencies": {
    "@bbbtech/storybook-formik": "2.5.0",
    "@faker-js/faker": "7.3.0",
    "@formatjs/cli": "5.0.2",
    "@formatjs/intl-relativetimeformat": "11.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-vite": "0.2.6",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/dom": "8.19.0",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/validator": "13.7.3",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/ui": "0.25.3",
    "babel-plugin-react-intl": "8.2.25",
    "cljs-merge": "1.1.1",
    "eslint-config-custom": "*",
    "formik": "2.2.9",
    "intl-pluralrules": "1.3.1",
    "msw": "0.49.1",
    "msw-storybook-addon": "1.6.3",
    "postcss": "8.4.14",
    "prettier": "2.7.0",
    "storybook-addon-react-router-v6": "0.2.1",
    "ts-node": "10.8.1",
    "tslib": "2.4.0",
    "typescript": "4.7.3",
    "typescript-plugin-css-modules": "3.4.0",
    "use-debounce": "8.0.4",
    "vite": "3.2.3",
    "vite-plugin-dts": "1.7.1",
    "vitest": "0.25.3",
    "yup": "0.32.11"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
