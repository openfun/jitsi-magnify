{
  "name": "@jitsi-magnify/core",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "build-storybook",
    "compile-translations": "./i18n/compile-translations.js",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "lint": "eslint src",
    "format:check": "prettier --check src ",
    "format:write": "prettier --write src"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@faker-js/faker": "7.3.0",
    "@formatjs/cli": "5.0.2",
    "@formatjs/intl-relativetimeformat": "11.0.2",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.2",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/jest": "28.1.1",
    "@types/react": "18.0.12",
    "@types/styled-components": "5.1.25",
    "@types/validator": "13.7.3",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "babel-plugin-react-intl": "8.2.25",
    "cljs-merge": "1.1.1",
    "eslint": "8.17.0",
    "eslint-plugin-react": "7.30.0",
    "intl-pluralrules": "1.3.1",
    "jest": "28.1.1",
    "jest-css-modules": "2.1.0",
    "jest-environment-jsdom": "28.1.1",
    "postcss": "8.4.14",
    "prettier": "2.7.0",
    "rollup": "2.75.6",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "tslib": "2.4.0",
    "typescript": "4.7.3",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "module": "dist",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "include": [
    "src/*"
  ],
  "peerDependencies": {
    "axios": "0.27.2",
    "grommet": "2.24.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-intl": "6.0.4",
    "react-query": "3.39.1",
    "react-router-dom": "6.3.0",
    "styled-components": "5.3.5",
    "validator": "13.7.0"
  },
  "dependencies": {}
