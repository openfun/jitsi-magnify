{
  "name": "magnify-site",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src ",
    "format:write": "prettier --write src"
  },
  "dependencies": {
    "@fontsource/roboto": "4.5.8",
    "@jitsi/react-sdk": "1.1.0",
    "@openfun/magnify-components": "*",
    "@magnify/typescript-configs": "*",
    "eslint-config-custom": "*",
    "@tanstack/react-query": "4.7.2",
    "@tanstack/react-query-devtools": "4.8.0",
    "axios": "0.27.2",
    "grommet": "2.25.3",
    "grommet-icons": "4.8.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.4",
    "react-router-dom": "6.4.1",
    "react-scripts": "5.0.1",
    "stream": "npm:stream-browserify",
    "stream-browserify": "3.0.0",
    "styled-components": "5.3.5",
    "validator": "13.7.0",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "eslint-config-custom": "*",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.40",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/ui": "0.25.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "jest": "28.1.1",
    "jest-css-modules": "2.1.0",
    "jest-environment-jsdom": "28.1.1",
    "msw": "0.47.4",
    "prettier": "2.7.0",
    "ts-jest": "28.0.4",
    "typescript": "4.6.4",
    "vite": "3.2.3",
    "vitest": "0.25.3"
  }
}
