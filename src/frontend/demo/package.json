  "license": "MIT",
  "private": false,
  "dependencies": {
    "@jitsi-magnify/core": "0.1.0",
    "axios": "0.27.2",
    "grommet": "2.24.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.0.4",
    "react-query": "3.39.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "stream": "npm:stream-browserify",
    "stream-browserify": "3.0.0",
    "styled-components": "5.3.5",
    "validator": "13.7.0",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.17.0",
    "eslint-plugin-react": "7.30.0",
    "jest": "28.1.1",
    "jest-css-modules": "2.1.0",
    "jest-environment-jsdom": "28.1.1",
    "prettier": "2.7.0",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  },
  "scripts": {
    "compile-translations": "./i18n/compile-translations.js",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
