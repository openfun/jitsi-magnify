{
  "name": "sandbox",
  "private": true,
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "compile-translations": "../packages/core/i18n/compile-translations.cjs ./i18n/locales/*.json",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src ",
    "format:write": "prettier --write src",
    "build-theme": "cunningham -g css,ts -o src"
  },
  "dependencies": {
    "@jitsi/react-sdk": "1.4.0",
    "@openfun/cunningham-react": "2.9.4",
    "@openfun/jitsi-magnify": "*",
    "@tanstack/react-query": "5.59.16",
    "@tanstack/react-query-devtools": "5.59.16",
    "@types/styled-components": "5.1.34",
    "axios": "1.7.7",
    "grommet": "2.41.0",
    "grommet-icons": "4.12.1",
    "polished": "4.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "6.8.2",
    "react-router-dom": "6.27.0",
    "react-scripts": "5.0.1",
    "stream": "npm:stream-browserify",
    "stream-browserify": "3.0.0",
    "styled-components": "5.3.11",
    "validator": "13.12.0",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@lib/eslint-config": "*",
    "@lib/typescript-configs": "*",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "@vitejs/plugin-react": "4.3.3",
    "@vitest/ui": "2.1.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-compat": "6.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "jsdom": "25.0.1",
    "msw": "2.5.1",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.3",
    "vitest-dom": "0.1.1"
  }
}
