{
  "name": "sandbox",
  "private": true,
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "compile-translations": "../packages/core/i18n/compile-translations.cjs ./i18n/locales/*.json",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src ",
    "format:write": "prettier --write src"
  },
  "dependencies": {
    "@fontsource/roboto": "5.0.2",
    "@jitsi/react-sdk": "1.3.0",
    "@openfun/jitsi-magnify": "*",
    "@tanstack/react-query": "4.29.12",
    "@tanstack/react-query-devtools": "4.29.12",
    "@types/styled-components": "5.1.26",
    "axios": "1.4.0",
    "grommet": "2.32.1",
    "grommet-icons": "4.10.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.2",
    "react-router-dom": "6.11.2",
    "react-scripts": "5.0.1",
    "stream": "npm:stream-browserify",
    "stream-browserify": "3.0.0",
    "styled-components": "5.3.11",
    "validator": "13.9.0",
    "web-vitals": "3.3.2"
  },
  "devDependencies": {
    "@lib/eslint-config": "*",
    "@lib/typescript-configs": "*",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.16",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/ui": "0.31.3",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "jest": "29.5.0",
    "jest-css-modules": "2.1.0",
    "jest-environment-jsdom": "29.5.0",
    "msw": "1.2.1",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.3"
  }
}
