{
  "name": "@openfun/jitsi-magnify",
  "private": false,
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 -s public",
    "react-router-dom": "6.4.1",
    "test": "vitest run ",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "compile-translations": "./i18n/compile-translations.cjs",
    "extract-translations": "formatjs extract './**/*.ts*'  --ignore ./node_modules --ignore './**/*.d.ts' --ignore ./tsconfig.tsbuildinfo --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "build-theme": "cunningham -g css -o src"
  },
  "dependencies": {
    "@openfun/cunningham-react": "0.7.0",
    "formik": "2.4.1",
    "grommet": "2.32.1",
    "grommet-icons": "4.10.0",
    "keycloak-js": "21.1.1",
    "validator": "13.9.0",
    "yup": "1.2.0"
  },
  "peerDependencies": {
    "@fontsource/roboto": "5.0.2",
    "@jitsi/react-sdk": "1.3.0",
    "@tanstack/react-query": "4.29.12",
    "axios": "1.4.0",
    "react-intl": "6.4.2",
    "react-router-dom": "6.11.2",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@bbbtech/storybook-formik": "2.6.0",
    "@faker-js/faker": "8.0.2",
    "@formatjs/cli": "6.1.1",
    "@formatjs/intl-relativetimeformat": "11.2.2",
    "@lib/eslint-config": "*",
    "@lib/typescript-configs": "*",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@storybook/addon-actions": "7.0.18",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/cli": "7.0.18",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "7.0.18",
    "@storybook/react-vite": "7.0.18",
    "@storybook/testing-library": "0.1.0",
    "@testing-library/dom": "9.3.0",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/styled-components": "5.1.26",
    "@types/validator": "13.7.17",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/ui": "0.31.3",
    "babel-plugin-react-intl": "8.2.25",
    "cljs-merge": "1.1.1",
    "intl-pluralrules": "2.0.0",
    "msw": "1.2.1",
    "msw-storybook-addon": "1.8.0",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.18",
    "storybook-addon-react-router-v6": "1.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.5.2",
    "typescript": "5.0.4",
    "typescript-plugin-css-modules": "5.0.1",
    "use-debounce": "9.0.4",
    "vite": "4.3.9",
    "vite-plugin-dts": "2.3.0",
    "vitest": "0.31.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
