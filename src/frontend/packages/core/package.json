{
  "name": "@openfun/jitsi-magnify",
  "private": false,
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 -s public",
    "react-router-dom": "6.4.1",
    "test": "vitest run ",
    "test-watch": "vitest --ui",
    "coverage": "vitest run --coverage",
    "compile-translations": "./i18n/compile-translations.cjs",
    "extract-translations": "formatjs extract './**/*.ts*'  --ignore ./node_modules --ignore './**/*.d.ts' --ignore ./tsconfig.tsbuildinfo --ignore './**/*.stories.ts*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "build-theme": "cunningham -g css,ts -o src"
  },
  "dependencies": {
    "@tanstack/react-query-devtools": "5.59.16",
    "formik": "2.4.6",
    "grommet": "2.41.0",
    "grommet-icons": "4.12.1",
    "keycloak-js": "26.0.2",
    "validator": "13.12.0",
    "yup": "1.4.0"
  },
  "peerDependencies": {
    "@jitsi/react-sdk": "1.4.0",
    "@openfun/cunningham-react": "2.9.4",
    "@tanstack/react-query": "5.59.16",
    "axios": "1.7.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "6.8.2",
    "react-router-dom": "6.27.0",
    "styled-components": "<6"
  },
  "devDependencies": {
    "@bbbtech/storybook-formik": "3.0.0",
    "@faker-js/faker": "9.0.3",
    "@formatjs/cli": "6.3.2",
    "@formatjs/intl-relativetimeformat": "11.3.0",
    "@lib/eslint-config": "*",
    "@lib/typescript-configs": "*",
    "@openfun/cunningham-react": "2.9.4",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@storybook/addon-actions": "8.3.6",
    "@storybook/addon-essentials": "8.3.6",
    "@storybook/addon-interactions": "8.3.6",
    "@storybook/addon-links": "8.3.6",
    "@storybook/cli": "8.3.6",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "8.3.6",
    "@storybook/react-vite": "8.3.6",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/styled-components": "5.1.34",
    "@types/validator": "13.12.2",
    "@vitejs/plugin-react": "4.3.3",
    "@vitest/ui": "2.1.3",
    "babel-plugin-react-intl": "8.2.25",
    "cljs-merge": "1.1.1",
    "intl-pluralrules": "2.0.1",
    "jsdom": "25.0.1",
    "msw": "2.5.1",
    "msw-storybook-addon": "2.0.3",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "8.3.6",
    "storybook-addon-react-router-v6": "2.0.15",
    "styled-components": "5.3.11",
    "ts-node": "10.9.2",
    "tslib": "2.8.0",
    "typescript": "5.6.3",
    "typescript-plugin-css-modules": "5.1.0",
    "use-debounce": "10.0.4",
    "vite": "5.4.10",
    "vite-plugin-dts": "4.3.0",
    "vitest": "2.1.3",
    "vitest-dom": "0.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
